SELECT DISTINCT ?year ?title ?thumbnail ?index
WHERE {
  {%include "../../../includes/graphs.inc"%}
  GRAPH ?graph {
    <{{uri}}> a swrc:VirtualPosterShowcase ;
      swrc:year ?year ;
      swrc:eventTitle ?title ;
      agu:showcaseIndex ?index .
    OPTIONAL { <{{uri}}> foaf:thumbnail ?thumbnail . }
  }
}
LIMIT 1